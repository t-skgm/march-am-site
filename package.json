{
  "name": "march-am-site",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "dev:fn": "wrangler pages dev --compatibility-date 2023-05-18 -- npm run dev",
    "start": "astro dev",
    "build": "astro build",
    "astro": "astro",
    "sync": "astro sync",
    "postinstall": "patch-package",
    "lint": "eslint --ext .js,.astro src",
    "fmt:check": "prettier src/",
    "fmt": "prettier --write src/",
    "test": "vitest",
    "typecheck": "tsc -p . --noEmit",
    "codegen": "contentful-typescript-codegen --output src/infra/contentful/generated.d.ts"
  },
  "dependencies": {
    "@astrojs/mdx": "1.1.5",
    "@astrojs/preact": "3.0.1",
    "@astrojs/rss": "3.0.0",
    "@astrojs/sitemap": "3.0.3",
    "@astrojs/tailwind": "5.0.2",
    "@cloudflare/pages-plugin-vercel-og": "^0.1.0",
    "@contentful/rich-text-html-renderer": "16.3.0",
    "@fontsource/lora": "5.0.8",
    "astro": "3.5.5",
    "astro-seo": "0.8.0",
    "contentful": "9.3.5",
    "dayjs": "1.11.10",
    "patch-package": "8.0.0",
    "preact": "10.19.2",
    "react": "18.2.0",
    "rehype-sanitize": "6.0.0",
    "remark": "15.0.1",
    "remark-gfm": "4.0.0",
    "remark-slug": "7.0.1",
    "remark-toc": "9.0.0",
    "remeda": "1.29.0",
    "sass": "1.69.5",
    "tailwindcss": "3.3.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20231025.0",
    "@types/bluebird": "3.5.42",
    "@types/node": "20.9.1",
    "@types/react": "18.2.37",
    "@typescript-eslint/parser": "6.11.0",
    "bluebird": "3.7.2",
    "contentful-management": "11.5.4",
    "contentful-typescript-codegen": "3.4.0",
    "dotenv": "16.3.1",
    "eslint": "8.54.0",
    "eslint-config-standard-with-typescript": "40.0.0",
    "eslint-plugin-astro": "0.29.1",
    "front-matter": "4.0.2",
    "prettier": "3.1.0",
    "prettier-plugin-astro": "0.12.2",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vitest": "0.34.6",
    "wrangler": "3.16.0"
  }
