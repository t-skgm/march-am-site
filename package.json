{
  "name": "march-am-site",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "astro dev",
    "dev:fn": "wrangler pages dev --compatibility-date 2023-05-18 -- pnpm dev",
    "start": "astro dev",
    "build": "astro build",
    "astro": "astro",
    "sync": "astro sync",
    "lint": "eslint --ext .js,.astro src",
    "fmt:check": "prettier src/",
    "fmt": "prettier --write src/",
    "test": "vitest",
    "typecheck": "tsc -p . --noEmit",
    "ncu": "npx -p npm-check-updates  -c \"ncu\"",
    "ncu-u": "npx -p npm-check-updates  -c \"ncu -u\""
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/preact": "^4.1.3",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.4.2",
    "@cloudflare/pages-plugin-vercel-og": "^0.1.2",
    "@contentful/rich-text-html-renderer": "^17.1.6",
    "@fontsource/lora": "^5.2.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.6",
    "astro-seo": "0.8.4",
    "contentful": "^11.10.1",
    "dayjs": "^1.11.19",
    "hast-util-to-html": "^9.0.5",
    "mdast-util-to-hast": "^13.2.1",
    "mdast-util-toc": "^7.1.0",
    "patch-package": "8.0.1",
    "preact": "^10.28.2",
    "react": "^19.2.3",
    "rehype-sanitize": "6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-toc": "9.0.0",
    "remeda": "^2.30.0",
    "sanitize-html": "2.17.0",
    "tailwindcss": "^4.1.18",
    "unified": "^11.0.5"
