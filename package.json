{
  "name": "march-am-site",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "astro dev",
    "dev:fn": "wrangler pages dev --compatibility-date 2023-05-18 -- npm run dev",
    "start": "astro dev",
    "build": "astro build",
    "astro": "astro",
    "sync": "astro sync",
    "lint": "eslint --ext .js,.astro src",
    "fmt:check": "prettier src/",
    "fmt": "prettier --write src/",
    "test": "vitest",
    "typecheck": "tsc -p . --noEmit",
    "ncu": "npx -p npm-check-updates  -c \"ncu\"",
    "ncu-u": "npx -p npm-check-updates  -c \"ncu -u\""
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.3",
    "@astrojs/preact": "^4.1.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.2",
    "@cloudflare/pages-plugin-vercel-og": "^0.1.2",
    "@contentful/rich-text-html-renderer": "^17.1.0",
    "@fontsource/lora": "^5.2.6",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "astro": "^5.13.2",
    "astro-seo": "0.8.4",
    "contentful": "^11.7.15",
    "dayjs": "^1.11.13",
    "patch-package": "8.0.0",
    "preact": "^10.27.0",
    "react": "^19.1.1",
    "rehype-sanitize": "6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-toc": "9.0.0",
    "remeda": "^2.30.0",
    "tailwindcss": "^4.1.11",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250810.0",
    "@types/node": "^24.2.1",
    "@types/react": "19.1.9",
    "@typescript-eslint/parser": "^8.39.0",
    "contentful-management": "^11.54.4",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-config-love": "^121.0.0",
    "eslint-plugin-astro": "^1.3.1",
    "front-matter": "4.0.2",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.34.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.28.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.46.2"
  }
}
