{
  "name": "march-am-site",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "dev:fn": "wrangler pages dev --compatibility-date 2023-05-18 -- npm run dev",
    "start": "astro dev",
    "build": "astro build",
    "astro": "astro",
    "sync": "astro sync",
    "postinstall": "patch-package",
    "lint": "eslint --ext .js,.astro src",
    "fmt:check": "prettier src/",
    "fmt": "prettier --write src/",
    "test": "vitest",
    "typecheck": "tsc -p . --noEmit",
    "codegen": "contentful-typescript-codegen --output src/infra/contentful/generated.d.ts"
  },
  "dependencies": {
    "@astrojs/mdx": "0.16.2",
    "@astrojs/preact": "2.0.2",
    "@astrojs/rss": "2.1.1",
    "@astrojs/sitemap": "1.0.1",
    "@astrojs/tailwind": "3.0.1",
    "@cloudflare/pages-plugin-vercel-og": "^0.1.0",
    "@contentful/rich-text-html-renderer": "16.0.2",
    "@fontsource/lora": "5.0.1",
    "astro": "2.0.14",
    "astro-seo": "0.7.1",
    "contentful": "9.3.5",
    "dayjs": "1.11.7",
    "patch-package": "7.0.0",
    "preact": "10.15.1",
    "react": "^18.2.0",
    "rehype-sanitize": "5.0.1",
    "remark": "14.0.3",
    "remark-gfm": "^3.0.1",
    "remark-slug": "7.0.1",
    "remark-toc": "8.0.1",
    "remeda": "1.19.0",
    "sass": "1.62.1",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230518.0",
    "@types/bluebird": "3.5.38",
    "@types/node": "20.2.5",
    "@types/react": "^18.2.14",
    "@typescript-eslint/parser": "5.59.7",
    "bluebird": "3.7.2",
    "contentful-management": "10.35.4",
    "contentful-typescript-codegen": "3.4.0",
    "dotenv": "16.0.3",
    "eslint": "8.41.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-astro": "0.27.1",
    "front-matter": "4.0.2",
    "prettier": "2.8.8",
    "prettier-plugin-astro": "0.9.1",
    "ts-node": "10.9.1",
    "vitest": "0.31.1",
    "wrangler": "^3.1.1"
  }
}
