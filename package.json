    "front-matter": "4.0.2",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.34.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.50.1",
    "unist-util-visit": "5.0.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "wrangler": "^4.54.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.54.0"
  }
