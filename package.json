{
  "name": "march-am-site",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "dev:fn": "wrangler pages dev --compatibility-date 2023-05-18 -- npm run dev",
    "start": "astro dev",
    "build": "astro build",
    "astro": "astro",
    "sync": "astro sync",
    "lint": "eslint --ext .js,.astro src",
    "fmt:check": "prettier src/",
    "fmt": "prettier --write src/",
    "test": "vitest",
    "typecheck": "tsc -p . --noEmit",
    "ncu": "npx -p npm-check-updates  -c \"ncu\"",
    "ncu-u": "npx -p npm-check-updates  -c \"ncu -u\""
  },
  "dependencies": {
    "@astrojs/mdx": "^4.2.0",
    "@astrojs/preact": "^4.0.5",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^6.0.0",
    "@cloudflare/pages-plugin-vercel-og": "^0.1.2",
    "@contentful/rich-text-html-renderer": "^17.0.0",
    "@fontsource/lora": "^5.2.5",
    "astro": "^5.5.1",
    "astro-seo": "0.8.4",
    "contentful": "10.12.7",
    "dayjs": "^1.11.13",
    "patch-package": "8.0.0",
    "preact": "^10.26.4",
    "react": "18.3.1",
    "rehype-sanitize": "6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-toc": "9.0.0",
    "remeda": "^2.21.2",
    "sass": "^1.85.1",
    "tailwindcss": "3.4.4",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250312.0",
    "@types/bluebird": "3.5.42",
    "@types/node": "^22.13.10",
    "@types/react": "18.3.3",
    "bluebird": "3.7.2",
    "contentful-management": "^11.48.0",
    "dotenv": "^16.4.7",
    "eslint": "8.57.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-astro": "^1.3.1",
    "front-matter": "4.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.33.4",
    "ts-node": "10.9.2",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "wrangler": "^4.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.18.0"
