  "devDependencies": {
    "@cloudflare/workers-types": "4.20251228.0",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.2.1",
    "@types/react": "19.2.7",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/parser": "^8.50.1",
    "contentful-management": "^11.66.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.39.2",
