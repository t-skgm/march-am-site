{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "include": ["src/**/*", "script/**/*", "vitest.config.ts"],
  "exclude": ["functions/**/*"],
  "ts-node": {
    "esm": true,
    "compilerOptions": {
      "noUnusedLocals": false,
      "noUnusedParameters": false
    }
  }
}
